{
    "name": "atari-7800-sprite-editor",
    "version": "0.0.0",
    "private": true,
    "devDependencies": {
        "@types/node": "24.9.1",
        "typescript": "5.9.3",
        "@types/bun": "1.3.1"
    },
    "scripts": {
        "build": "bun build --outfile public/app.js --target browser --watch --no-clear-screen public/app.ts",
        "sass": ".dev/dart-sass/sass --watch --no-source-map public/app.scss public/app.css",
        "start": "bun run --watch --no-clear-screen server.ts"
    },
    "module": "public/Canvas.ts",
    "type": "module"
}
